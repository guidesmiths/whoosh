{
  "extends": ["eslint-config-airbnb-base", "prettier"],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "2015"
  },
  "rules": {
    "consistent-return": 0,
    "prefer-rest-params": 0,
    "no-param-reassign": 0,
    "no-shadow": [0, { "allowed": ["err"] }],
    "no-use-before-define": 0
  }
}
