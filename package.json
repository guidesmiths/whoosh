{
  "name": "whoosh",
  "version": "2.0.1",
  "description": "A streaming sftp client",
  "main": "index.js",
  "scripts": {
    "test": "zUnit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "coverage": "nyc --report html --reporter lcov --reporter text-summary zUnit",
    "docker": "docker-compose up -d",
    "prepare": "husky install",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "keywords": [
    "sftp",
    "streaming",
    "client"
  ],
  "engines": {
    "node": ">=14"
  },
  "author": "GuideSmiths",
  "license": "ISC",
  "devDependencies": {
    "async": "^3.2.4",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "nyc": "^15.1.0",
    "prettier": "2.6.2",
    "zunit": "^3.2.1"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "ssh2": "^1.10.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guidesmiths/whoosh.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/whoosh/issues"
  },
  "zUnit": {
    "pollute": true
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.js": "eslint --fix ."
  },
  "homepage": "https://guidesmiths.github.io/whoosh/"
}
